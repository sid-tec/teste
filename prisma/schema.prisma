// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL_LITE")
}

model Servidor {
  key           String  @id
  nomeCurto     String
  nomeCompleto  String
  //
  siape         String  @unique
  cpf           Int?    @unique
  celular       Int?    @unique
  email         String? @unique
  sala          String?
  //
  masculino     Boolean
  professor     Boolean
  admissao      String
  aposentadoria String?
}
